<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
<ItemGroup>
  <Content Include="$(MSBuildThisFileDirectory)..\..\runtimes\win-x64\native\RobotRaconteurNETNative.dll" Condition=" '$(OS)' == 'Windows_NT' AND '$(Platform)' == 'x64' ">
    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
  </Content>    
  <Content Include="$(MSBuildThisFileDirectory)..\..\runtimes\win-x86\native\RobotRaconteurNETNative.dll" Condition=" '$(OS)' == 'Windows_NT' AND ('$(Platform)' == 'x86' OR '$(Platform)' == 'AnyCPU' ) ">
    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
  </Content>  
</ItemGroup>
<ItemGroup>
  <AvailableItemName Include="RobotRaconteurGenCSharp" />   
  <AvailableItemName Include="RobotRaconteurGenInclude" />   
</ItemGroup>
		
<Target Name="BeforeBuild">
	<Message Text="RobotRaconteurGenCSharp: @(RobotRaconteurGenCSharp->'&quot;%(fullpath)&quot;', ' ')"/>
	<Message Text="RobotRaconteurGenInclude: @(RobotRaconteurGenInclude->'--include &quot;%(fullpath)&quot;', ' ')"/>
</Target>

<Target Name="UpdateRobotRaconteurGenFiles" DependsOnTargets="_UpdateRobotRaconteurGenFiles" Condition="'@(RobotRaconteurGenCSharp)' != '' AND '$(OS)' == 'Windows_NT'">
  <ItemGroup>
    <Compile Include="$(BaseIntermediateOutputPath)RobotRaconteurGenCSGeneratedFile.g.cs" />    
  </ItemGroup>
</Target>
<Target Name="_UpdateRobotRaconteurGenFiles" Inputs="$(MSBuildProjectFile);@(RobotRaconteurGenCSharp)"
  Outputs="$(BaseIntermediateOutputPath)RobotRaconteurGenCSGeneratedFile.g.cs">  
  <Exec Command="&quot;$(MSBuildThisFileDirectory)..\..\tools\RobotRaconteurGen.exe&quot; --thunksource --lang=csharp --outfile=$(BaseIntermediateOutputPath)RobotRaconteurGenCSGeneratedFile.g.cs @(RobotRaconteurGenInclude->'--include &quot;%(fullpath)&quot;', ' ') @(RobotRaconteurGenCSharp->'&quot;%(fullpath)&quot;',' ')"/>
</Target>

<PropertyGroup>
  <CoreCompileDependsOn>UpdateRobotRaconteurGenFiles;$(CoreCompileDependsOn)</CoreCompileDependsOn>
</PropertyGroup>
		
</Project>